{
  "name": "gpuo",
  "version": "1.0.0",
  "description": "A tool to automate git cherry-pick and push operations",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "npx tsx backend/server-express.ts",
    "dev:frontend": "vite",
    "build": "tsc && vite build",
    "build:backend": "tsc backend/server-express.ts --outDir dist",
    "start": "node dist/server-express.js",
    "db:setup": "npx tsx backend/db/database.ts"
  },
  "keywords": ["git", "cherry-pick", "automation", "typescript"],
  "author": "WahidinAji",
  "license": "MIT",
  "dependencies": {
    "@tanstack/react-query": "^5.17.0",
    "better-sqlite3": "^9.2.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    "tailwind-merge": "^2.2.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.3",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  },
  "type": "module"
}
